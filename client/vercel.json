{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "buildCommand": "cd client && npm ci --force && npm run build",
  "installCommand": "cd client && npm install --force",
  "framework": "vite"
}